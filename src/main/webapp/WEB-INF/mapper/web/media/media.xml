<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="web.brd.media">

	<select id="selectMediaList" parameterType="Map" resultType="Map">
		SELECT 
			ME_NUM,    		
			ME_NAME,
			ME_TITLE,       
			ME_DES,        
			ME_IMG,        
			ME_DATE_TAKEN,        
			ME_CONT
		FROM
			WE_CG
	</select>
</mapper>